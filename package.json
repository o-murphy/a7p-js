{
  "name": "a7p-js",
  "version": "1.0.6",
  "description": "Wrapper for .a7p files",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    },
    "./validate": {
      "import": "./dist/validate.js",
      "types": "./dist/validate.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./profedit": {
      "import": "./dist/profedit.js",
      "types": "./dist/profedit.d.ts"
    }
  },
  "scripts": {
    "build:proto": "npx pbjs -t static-module -w es6 -o src/profedit.js src/proto/profedit.proto && npx pbts -o src/profedit.d.ts src/profedit.js && node scripts/fix-imports.mjs",
    "build:ts": "tsc",
    "build:copy": "npx cpy 'src/profedit.*' 'dist/'",
    "build": "yarn build:ts && yarn build:copy"
  },
  "author": "o-murphy",
  "license": "ISC",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o-murphy/a7p-js.git"
  },
  "keywords": [
    "ballistic",
    "calculator",
    "js",
    "typescript",
    "es6"
  ],
  "bugs": {
    "url": "https://github.com/o-murphy/a7p-js/issues"
  },
  "homepage": "https://github.com/o-murphy/a7p-js#readme",
  "dependencies": {
    "base64-js": "^1.5.1",
    "crypto-js": "^4.2.0",
    "protobufjs": "^7.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.1.2",
    "@types/base64-js": "^1.5.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/yup": "^0.32.0",
    "babel-jest": "^30.1.2",
    "cpy-cli": "^6.0.0",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.8.3"
  }
}
